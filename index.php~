<?php
if (isset($_GET['page'])) {
	switch ($_GET['page']) {
		case "users":
			require("./access/users.php");
			break;
		case "services":
			require("./access/services.php");
			break;
		case "team":
			require("./access/team.php");
			break;
		case "home":
			require("./access/home.php");
			break;
		default:
			header("Location: ./?page=home");
	}
} else header("Location: ./?page=home");
?>
